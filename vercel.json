{
  "version": 2,
  "builds": [
    {
      "src": "frontend",
      "use": "@vercel/static-build",
      "config": {
        "framework": "vue"
      }
    },
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/search",
      "dest": "api/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/public/index.html"
    }
  ]
}